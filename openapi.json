  "paths" : {
    "/organization" : {
      "get" : {
        "summary" : "get a list of portal organizations",
        "description" : "Search for available organizations in the portal\n",
        "operationId" : "searchOrganization",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "name",
          "in" : "query",
          "description" : "pass an optional search string for looking up organization by name",
          "required" : false,
          "type" : "string"
        }, {
          "name" : "city",
          "in" : "query",
          "description" : "city optional parameter",
          "required" : false,
          "type" : "string"
        }, {
          "name" : "state",
          "in" : "query",
          "description" : "state optional parameter",
          "required" : false,
          "type" : "string"
        }, {
          "name" : "zip",
          "in" : "query",
          "description" : "zip optional parameter",
          "required" : false,
          "type" : "string"
        } ],
        "responses" : {
          "200" : {
            "description" : "search results matching criteria",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Organization"
              }
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    },
    "/profiles" : {
      "get" : {
        "summary" : "get a list of profiles",
        "description" : "Search for available profiles\n",
        "operationId" : "searchProfiles",
        "produces" : [ "application/json" ],
        "parameters" : [ {
          "name" : "guidId",
          "in" : "path",
          "description" : "uuid of profile",
          "required" : true,
          "type" : "string",
          "format" : "uuid"
        } ],
        "responses" : {
          "200" : {
            "description" : "search results matching criteria",
            "schema" : {
              "type" : "array",
              "items" : {
                "$ref" : "#/definitions/Profile"
              }
            }
          },
          "400" : {
            "description" : "bad input parameter"
          }
        }
      }
    }
  },
  "definitions" : {
    "Organization" : {
      "type" : "object",
      "required" : [ "id", "name" ],
      "properties" : {
        "id" : {
          "type" : "string",
          "format" : "uuid",
          "example" : "d290f1ee-6c54-4b01-90e6-d701748f0851"
        },
        "name" : {
          "type" : "string",
          "example" : "Healthcare Intelligence"
        }
      }
    },
    "Profile" : {
      "type" : "object",
