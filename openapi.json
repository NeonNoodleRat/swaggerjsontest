          "type" : "string"
        }, {
          "name" : "state",
          "in" : "query",
          "description" : "state of an org",
          "required" : false,
          "type" : "string"
        }, {
          "name" : "zip",
          "in" : "query",
          "description" : "zip of an org",
          "required" : false,
          "type" : "string"
